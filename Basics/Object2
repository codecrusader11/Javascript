//DESTRUCTURING AN OBJECT

const newObj={
    name:"tata",
    address:"sector-7",
    email:"abc@gmail.com"
}
const {address}=newObj;
console.log(address)

const {email:contact}=newObj;//can even change the name of the property and writing 'email' won't work
console.log(contact)